{
  "name": "datocms-plugin-image-thumbnail",
  "description": "If you have a text URL field, and the content ends in jpg, jpeg, gif or png, it will show a preview of the image under the field.",
  "homepage": "https://github.com/joeybk85/datocms-plugin-image-thumbnail",
  "private": false,
  "version": "0.1.0",
  "author": "Joey Kilbride <joeybk85@gmail.com>",
  "type": "module",
  "keywords": [
    "datocms-plugin"
  ],
  "datoCmsPlugin": {
    "title": "Datocms Plugin Thumbnail",
    "coverImage": "docs/cover-1200x800.png",
    "previewImage": "docs/cover-1200x800.png",
    "entryPoint": "dist/index.html",
    "permissions": []
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "datocms-plugin-sdk": "*",
    "datocms-react-ui": "*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
